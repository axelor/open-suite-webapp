{
    "palette": {
      "blue": "#6C67FA",
      "green": "#3ECF8E",
      "purple": "#C354F2",
      "red": "#DD514C",
      "yellow": "#FFBE4E",
      "cyan": "#26C6F9",
  
      "white": "#FFF",
      "black": "#333",
  
      "body_color": "#2f3945",
      "emphasis_color": "#FF0000",
      "body_bg": "",
      "secondary_bg": "",
      "secondary_color": "",
      "tertiary_bg": "",
      "tertiary_color": "",
  
      "primary": "#262e68",
      "secondary": "#a9c1e3",
      "success": "",
      "warning": "",
      "danger": "",
      "info": "",
      "light": "",
      "dark": "#2f3945"
    },
    "border": {
      "color": "#EAEAFE"
    },
    "typography": {
      "body": {
        "fontSize": "15px",
        "fontFamily": "",
        "fontWeight": "",
        "lineHeight": ""
      }
    },
    "components": {
      "Shell": {
        "sidebar": {
          "bg": "#FFFFFF",
          "border": {
          "width": "1px",
          "color": "#262e68",
          "style": "solid",
          "radius": ""
        }
        },
        "LoginForm": {
        "border": {
          "width": "1px",
          "color": "#DF0606",
          "style": "solid",
          "radius": "50px"
        },
        "bg": "#DF0606",
        "shadow": "0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19)"
      }
      },
      "Panel": {
        "header": {
          "bg": "#FBFBFF",
          "border": {
            "width": 0
          }
        },
        "title": {
          "padding": "0.25rem 0.5rem"
        }
      },
      "Dropdown": {
        "border": {
          "color": "#EAEAFE"
        },
        "item_hover": {
          "bg": "#F8F8FF"
        },
        "item_active": {
          "bg": "primary",
          "color": "#fff"
        },
        "divider": {
          "bg": "#EAEAFE"
        }
      },
      "NavMenu": {
        "item": {
          "border": {
            "radius": "1rem"
          }
        },
        "item_hover": {
          "bg": "var(--bs-primary-bg-subtle)"
        },
        "item_active": {
          "bg": "rgba(var(--bs-primary-rgb), 0.8)",
          "color": "white"
        },
        "icon": {
          "bg": "transparent"
        },
        "icon_hover": {
          "bg": "transparent"
        },
        "icon_active": {
          "color": "white"
        }
      },
      "NavTabs": {
        "item": {
          "padding": "0.25rem 0.5rem"
        },
        "text": {
          "transform": "none"
        },
        "icon": {
          "bg": "transparent"
        },
        "icon_hover": {
          "bg": "transparent"
        },
        "icon_active": {
          "bg": "transparent"
        }
      },
      "CommandBar": {
        "button": {
          "padding": "0.35rem 0.45rem"
        },
        "button_hover": {
          "bg": "#F8F8FF"
        },
        "button_active": {
          "bg": "#EAEAFE"
        }
      },
      "Table": {
        "bg": "#FFF",
        "border": {
          "color": "#EFEDFE"
        },
        "header": {
          "bg": "#FFF"
        },
        "row_odd": {
          "bg": "#FBFBFF"
        },
        "row_hover": {
          "bg": "#F0F0FD"
        },
        "row_active": {
          "bg": "#F0F0FD"
        },
        "cell_active": {
          "bg": "#E8E8FD"
        },
        "cell": {
          "padding": "0.375rem 0.5rem"
        }
      },
      "Button": {
        "paddingX": "0.5rem",
        "paddingY": "0.25rem"
      },
      "Badge": {
        "padding": "0.25rem 0.5rem",
        "border": {
          "radius": "1rem"
        }
      },
      "Form": {
        "padding": "1rem",
        "gap": "0.5rem 0.75rem"
      },
      "Input": {
        "padding": "calc(0.25rem + 1px) 0 0.25rem 0",
        "focus": {
          "shadow": "none",
          "border_width": "0 0 1px 0"
        },
        "invalid_focus": {
          "shadow": "none",
          "border_width": "0 0 1px 0"
        },
        "placeholder": {
          "color": "var(--bs-tertiary-color)"
        },
        "invalid": {
          "border_width": "0 0 1px 0"
        },
        "border": {
          "radius": 0
        },
        "border_width": "0 0 1px 0"
      }
    }
  }